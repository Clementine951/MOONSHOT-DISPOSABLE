{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "**/**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "./src/**/*", 
    "node_modules"
  ]
}
